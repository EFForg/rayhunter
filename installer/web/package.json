    "svelte-check": "^4.1.6",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@eslint/js": "^9.28.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@tailwindcss/vite": "^4.1.7",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-svelte": "^3.9.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.4.0",
    "tailwindcss": "^4.1.7",
    "typescript-eslint": "^8.33.0",
    "vitest": "^3.1.4"
  }
